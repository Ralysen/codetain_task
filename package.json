{
  "name": "charger",
  "version": "1.0.0",
  "description": "Node.js Rest Api for charging station system with Express, Sequalize & MySQL",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "migration:show": "npm typeorm migration:show",
    "migration:create": "node scripts/migration-create.js",
    "migration:revert": "npm typeorm migration:revert"
  },
  "keywords": [
    "nodejs",
    "express",
    "rest",
    "api",
    "sequalize",
    "mysql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.2",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "typescript": "^5.3.2"
  }
}
